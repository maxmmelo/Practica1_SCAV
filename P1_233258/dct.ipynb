   "source": [
    "# Impportem  de la llibreria scipy, la fft (Fast Fourier Transform) que té un mètode per a fer la dct. \n",
    "from scipy import fft\n",
    "  \n",
    "# Using scipy.fft.dct() method\n",
    "gfg = fft.dct([-1,2,3,4])\n",
    "  \n",
    "print(gfg)\n",
    "\n",
    "#veiem que a més és invertible:\n",
    "igfg = fft.idct(gfg)\n",
    "print(igfg)"
   ]
